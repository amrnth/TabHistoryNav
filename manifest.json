{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Tab History Navigation",
    "permissions": ["tabs", "activeTab"],
    "background":{
        "service_worker":"background.js"
    },
    "commands":{
        "go-back-history":{
            "suggested_key":{
                "default": "Alt+Z",
                "mac": "MacCtrl+Z"
            },
            "description": "Go back in history"
        },
        "go-forward-history":{
            "suggested_key":{
                "default": "Alt+Shift+Z",
                "mac": "MacCtrl+Shift+Z"
            },
            "description": "Go forward in history"
        }
    }
}